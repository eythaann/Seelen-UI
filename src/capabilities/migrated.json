{
  "$schema": "../gen/schemas/windows-schema.json",
  "identifier": "migrated",
  "description": "permissions that were migrated from v1. windows: [settings, wm]",
  "local": true,
  "windows": [
    "QHNlZWxlbi9zZXR0aW5ncw"
  ],
  "permissions": [
    "fs:allow-read-text-file",
    "fs:allow-write-text-file",
    "fs:allow-exists",
    "fs:allow-mkdir",
    "fs:allow-read-dir",
    "fs:allow-copy-file",
    "fs:allow-remove",
    {
      "identifier": "fs:scope",
      "allow": [
        {
          "path": "**"
        },
        {
          "path": "**/*"
        },
        {
          "path": "/**/*"
        }
      ]
    },

    "process:allow-restart",
    "process:allow-exit"
  ]
}
